<div id="<%= dom_id project %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= project.title %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Description:</strong>
    <%= project.description %>
  </p>
  <img src="<%= project.image_url %>" alt="Project Image" class="w-full h-auto mb-5">

  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= project.user_id %>
  </p>

</div>
